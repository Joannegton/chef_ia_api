# Environment variables template
# Copy this file to .env and fill in your actual values
# ⚠️  NUNCA commite este arquivo com valores reais no git!

# ========================================
# APPLICATION ENVIRONMENT
# ========================================
NODE_ENV=development
PORT=3000

# ========================================
# GOOGLE GEMINI API CONFIGURATION
# Get from: https://makersuite.google.com/app/apikey
# ========================================
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_TIMEOUT=30000
GEMINI_RETRY_ATTEMPTS=3

# ========================================
# SUPABASE CONFIGURATION
# Get from: https://supabase.com
# ========================================
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# ========================================
# CORS CONFIGURATION
# ========================================
# Em desenvolvimento: separadas por vírgula
# Em produção: configure as URLs reais do seu frontend
CORS_ORIGINS=http://localhost:3000,http://localhost:8080,http://localhost:5000

# ========================================
# RATE LIMITING
# ========================================
RATE_LIMIT_TTL=60000
RATE_LIMIT_MAX=100

# ========================================
# CACHE CONFIGURATION
# ========================================
CACHE_TTL=3600000
CACHE_MAX_ITEMS=100

# ========================================
# SECURITY
# ========================================
JWT_SECRET=your-jwt-secret-here